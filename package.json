    "@reduxjs/toolkit": "^1.9.1",
    "@skalenetwork/ima-js": "adiled/ima-js#2.0-hotfix-signer",
    "@skaleproject/skale.js": "^0.2.1",
    "@tanstack/query-core": "^4.20.4",
    "@tanstack/react-query": "^4.20.9",
    "buffer": "^6.0.3",
    "connectkit": "^1.1.1",
    "ethers": "^5.7.2",
    "framer-motion": "^9.0.1",
    "humanize-duration": "^3.28.0",
    "lodash": "^4.17.21",
    "lodash.debounce": "^4.0.8",
    "pretty-bytes": "^6.0.0",
    "process": "^0.11.10",
    "radix-ui": "^1.0.1",
    "react": "^18.2.0",
    "react-charts": "^3.0.0-beta.51",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
